{
  "name": "mevn-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "export NODE_ENV=production && babel dev-server -d prod-server && vue-cli-service build",
    "lint": "vue-cli-service lint",
    "dev": "concurrently \"export NODE_ENV=development\" \"babel dev-server -d prod-server --watch\" \"nodemon prod-server/index.js\" \"npm run serve\"",
    "start": "export NODE_ENV=production && node prod-server/index.js"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "babel-runtime": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "core-js": "^2.6.11",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "mongoose": "^5.12.7",
    "vue": "^2.6.11",
    "vue-markdown": "^2.1.2",
    "vue-router": "^3.1.6",
    "vuetify": "^2.2.21",
    "vuex": "^3.1.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@vue/cli-plugin-babel": "^3.12.1",
    "@vue/cli-plugin-eslint": "^3.1.1",
    "@vue/cli-service": "^4.5.13",
    "babel-eslint": "^10.1.0",
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.9",
    "sass": "^1.19.0",
    "sass-loader": "^8.0.0",
    "vue-cli-plugin-vuetify": "~2.0.5",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.3.0"
  },
  "eslintConfig": {
    "rules": {
      "no-console": "off"
    }
  }
}
